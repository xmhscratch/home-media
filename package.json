{
  "name": "home-media",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "frontend:start": "ng serve --host=0.0.0.0 --configuration=development",
    "frontend:build": "ng build --verbose --progress=false",
    "frontend:watch": "ng build --watch --configuration=development",
    "frontend:format": "npx prettier --write ./frontend/app ./frontend/environments ./frontend/polyfills ./frontend/app.controller.ts ./frontend/app.module.ts ./frontend/main.server.ts ./frontend/main.ts ./frontend/polyfills.ts ./frontend/server.ts",
    "frontend:test": "ng test",
    "frontend:serve:ssr": "node dist/frontend/server/server.mjs",
    "nest": "nest",
    "backend:build": "nest --path=./tsconfig.nest.json build",
    "backend:format": "prettier --write \"backend/**/*.ts\"",
    "backend:start": "nest --sourceRoot='./backend' --path=./tsconfig.nest.json start",
    "backend:start:dev": "nest --sourceRoot='./backend' --path=./tsconfig.nest.json start --watch",
    "backend:start:debug": "nest --sourceRoot='./backend' --path=./tsconfig.nest.json start --debug --watch",
    "backend:start:prod": "node ./dist/backend/main",
    "backend:lint": "eslint \"backend/{src,apps,libs,test}/**/*.ts\" --fix",
    "backend:test": "jest",
    "backend:test:watch": "jest --watch",
    "backend:test:cov": "jest --coverage",
    "backend:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "backend:test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:build:gonode": "docker build --no-cache --progress=plain -t localhost:5000/gonode:latest ./ci/gonode",
    "docker:build:sysenv": "docker build --no-cache --progress=plain -t localhost:5000/sysenv:latest ./ci/sysenv",
    "docker:build:hms": "docker buildx build --no-cache --progress=plain --file=./hms.Dockerfile -t localhost:5000/hms:latest ./",
    "docker:build:app": "docker buildx build --no-cache --progress=plain --file=app.Dockerfile --output type=local,dest=./dist/app/ ./",
    "docker:build:minikube": "docker buildx build --no-cache --progress=plain --file=./minikube.Dockerfile --output type=local,dest=./dist/bin/ ./",
    "docker:build:webos": "docker buildx build --no-cache --progress=plain --file=./webos.Dockerfile --output type=local,dest=./dist/iso/ ./",
    "docker:save:gonode": "docker save localhost:5000/gonode:latest > ./dist/docker/gonode.tar",
    "docker:save:sysenv": "docker save localhost:5000/sysenv:latest > ./dist/docker/sysenv.tar",
    "docker:save:hms": "docker save localhost:5000/hms:latest > ./dist/docker/hms.tar",
    "docker:load:gonode": "docker load --input ./dist/docker/gonode.tar",
    "docker:load:sysenv": "docker load --input ./dist/docker/sysenv.tar",
    "docker:load:hms": "docker load --input ./dist/docker/hms.tar",
    "docker:inspect:sysenv": "docker run --rm -it --entrypoint sh localhost:5000/sysenv:latest",
    "docker:inspect:hms": "docker run --rm -it --entrypoint sh localhost:5000/hms:latest",
    "swarmpit": "dotenvx run -- docker stack deploy --prune --resolve-image always -c ./ci/swarmpit/docker-stack.yml swarmpit",
    "redis": "dotenvx run -- docker stack deploy --prune --resolve-image always -c ./ci/redis/docker-stack.yml redis",
    "logstash": "dotenvx run -- docker stack deploy --prune --resolve-image always -c ./ci/logstash/docker-stack.yml logstash",
    "hms:deploy": "dotenvx run -- docker stack deploy --prune --resolve-image always -c ./ci/hms/docker-stack.yml hms",
    "hms:destroy": "dotenvx run -- docker stack rm hms",
    "hms:update": "dotenvx run -- docker service update --force $@",
    "hms:up": "dotenvx run -- docker service update --replicas 1 $@",
    "hms:down": "dotenvx run -- docker service update --replicas 0 $@",
    "minikube": "minikube dashboard --port 33333",
    "startup": "cp -vf ./ci/minikube.service /etc/systemd/system/; systemctl enable minikube; systemctl start minikube;",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/ssr": "^18.2.12",
    "@dotenvx/dotenvx": "^1.36.0",
    "@fastly/performance-observer-polyfill": "^2.0.0",
    "@grpc/grpc-js": "^1.13.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/websockets": "^10.0.0",
    "@primeng/themes": "^18.0.2",
    "amqp-connection-manager": "^4.1.14",
    "array.prototype.flat": "^1.3.3",
    "better-sqlite3": "^11.7.2",
    "bson-objectid": "^2.0.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "es5-shim": "^4.6.7",
    "express": "^4.18.2",
    "filesize": "^10.1.6",
    "flatory": "^0.0.6",
    "globalthis": "^1.0.4",
    "ioredis": "^5.4.2",
    "kafkajs": "^2.2.4",
    "lodash-es": "^4.17.21",
    "lru-cache": "^11.0.2",
    "mqtt": "^5.10.4",
    "nats": "^2.29.3",
    "object-hash": "^3.0.0",
    "object.fromentries": "^2.0.8",
    "primeicons": "^7.0.0",
    "primeng": "^18.0.2",
    "queue-microtask": "^1.2.3",
    "range-parser": "^1.2.1",
    "reflect-metadata": "^0.2.0",
    "regex": "^6.0.1",
    "rss-parser": "^3.13.0",
    "rxjs": "~7.8.0",
    "sql.js": "^1.12.0",
    "string.prototype.matchall": "^4.0.12",
    "string.prototype.replaceall": "^1.0.10",
    "tailwindcss": "^3.4.17",
    "tailwindcss-primeui": "^0.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular-eslint/builder": "19.3.0",
    "@angular-eslint/eslint-plugin": "19.3.0",
    "@angular-eslint/eslint-plugin-template": "19.3.0",
    "@angular-eslint/schematics": "19.3.0",
    "@angular-eslint/template-parser": "19.3.0",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/mapped-types": "^2.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.18.0",
    "@types/object-hash": "^3.0.6",
    "@types/sql.js": "^1.4.9",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "autoprefixer": "^10.4.20",
    "bufferutil": "^4.0.9",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "jasmine-core": "~5.2.0",
    "jest": "^29.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.1",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.3.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.5.2",
    "utf-8-validate": "^6.0.5",
    "video.js": "^8.23.0"
  }
}
